{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint . --ext .ts  --fix",
    "i-nstall": "npm i",
    "clean": "rm -rf build/ || true",
    "build": "npm run clean && npx tsc",
    "depoly": "chmod +x bin/deploy.sh && bin/deploy.sh",
    "env-variable": "export NODE_TLS_REJECT_UNAUTHORIZED='0'",
    "up": "db-migrate up",
    "down": "env=test db-migrate down",
    "start": "node ./build/index.js",
    "npm start": "NODE_DEBUG=* node index.js",
    "test": "env=development npm run up up && npm run build && jasmine && npm run down",
    "startx": "npm run build && node build/index.js",
    "dev": "npm run prettier && npm run lint && npm run test && npm run start",
    "dev-server": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./build/index.js\""
  },
